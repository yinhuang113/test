<%= simple_form_for(@mall) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  
  <%= f.association :account, as: :hidden %>
  <%= f.input :name %>
  <%= f.input :email %>
  <%= f.input :website %>
  <%= f.input :phone %>
  <%= f.input :address %>
  <%= f.input :city %>
  <%= f.input :state %>
  <%= f.input :zipcode %>
  <%= f.input :latitude %>
  <%= f.input :longitude %>
  
  <%= f.button :submit, class: "btn btn-large btn-primary" %>
<% end %>
