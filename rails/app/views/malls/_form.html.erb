<%= form_for(@mall) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  
  <%= f.label :name %>
  <%= f.text_field :name %>
  
  <%= f.label:email %>
  <%= f.text_field :email %>
  
  <%= f.label:phone %>
  <%= f.text_field :phone %>
  
  <%= f.label :address %>
  <%= f.text_field :address %>
  
  <%= f.label :city %>
  <%= f.text_field :city %>
  
  <%= f.label:state %>
  <%= f.text_field :state %>
  
  <%= f.label:zipcode %>
  <%= f.text_field :zipcode %>
  
  <%= f.label:latitude %>
  <%= f.text_field :latitude %>
  
  <%= f.label:longitude %>
  <%= f.text_field :longitude %>
  
  <%= f.submit yield(:submit_title), :class => "btn btn-large btn-primary" %>
<% end %>
