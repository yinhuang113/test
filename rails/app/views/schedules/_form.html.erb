<%= simple_form_for([@mall, @schedule]) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  
  <%= f.association :mall, as: :hidden %>
  <%= f.input :name %>
  <%= f.input :priority %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  
  <%= f.button :submit, class: "btn btn-large btn-primary" %>
<% end %>
